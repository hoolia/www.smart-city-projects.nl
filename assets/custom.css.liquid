/* comments area css */
.comment-area {
  margin-top: 70px;
}
.comments-heading > h3 {
  border-top: 1px solid {{ borders_color }};
  font-family: {{ header_family }};
  font-size: 24px;
  margin: 0 0 50px;
  padding: 22px 0 0;
  text-transform: uppercase;
}
.comment-list-image {
  float: left;
  margin-right: 15px;
}
.comment-content-wrap > p {
  margin-top: 15px;
}
.comment-content-wrap {
  border: 1px solid #ddd;
  color: #42414f;
  font-size: 12px;
  line-height: 1;
  margin: 0 0 15px 63px;
  padding: 10px;
  position: relative;
}
.comment-content-wrap::before {
  background: {{ color_skin_2 }} none repeat scroll 0 0;
  border-bottom: 1px solid{{ borders_color }};
  border-left: 1px solid{{ borders_color }};
  content: "";
  display: block;
  height: 10px;
  left: -5px;
  position: absolute;
  top: 10px;
  transform: rotate(45deg);
  width: 10px;
}
.comments-list ul li {
  margin-bottom: 25px;
}
.threaded-comments {
  margin-left: 50px;
}
.comment-respond {
  margin-top: 65px;
}
h3.comment-reply-title {
  border-top: 1px solid {{ borders_color }};
  font-family: {{ base_family }};
  font-size: 24px;
  margin: 0 0 50px;
  padding: 22px 0 0;
  text-transform: uppercase;
}
span.email-notes {
  color: {{ color_body }};
  display: block;
  font-size: 12px;
  margin-bottom: 10px;
}
.comment-respond input[type="text"],
.comment-respond input[type="email"] {
  background: {{ color_skin_2 }} none repeat scroll 0 0;
  border: 1px solid {{ borders_color }};
  border-radius: 0;
  height: 32px;
  margin-bottom: 15px;
  padding: 0 0 0 10px;
  width: 100%;
}
.comment-respond textarea#message, .comment-respond textarea#CommentBody {
  background: {{ color_skin_2 }} none repeat scroll 0 0;
  border: 1px solid {{ borders_color }};
  border-radius: 0;
  height: 130px;
  max-width: 100%;
  padding: 10px;
  width: 100%;
}
.comment-respond input[type="submit"] {
  background: {{ headings_color }} none repeat scroll 0 0;
  border: medium none;
  border-radius: 0;
  box-shadow: none;
  color: {{ color_skin_2 }};
  display: inline-block;
  font-family: {{ base_family }};
  font-size: 12px;
  font-weight: 400;
  height: 40px;
  line-height: 40px;
  margin-top: 20px;
  padding: 0 15px;
  text-shadow: none;
  text-transform: capitalize;
  transition: all 0.3s ease 0s;
  white-space: nowrap;
}
.comment-respond input[type="submit"]:hover {
  background-color: {{ color_skin }};
}

#checkout-cart .pull-right .additional-checkout-button {
