<script>
  document.querySelector('.mobile-menu .oc-menu-bar .right').addEventListener('click', function(e){
    this.closest('.common-home .header-inner').classList.toggle('whiteHeader');
    this.querySelector('i').classList.toggle('icon-menu')
    this.querySelector('i').classList.toggle('icon-close')
})
</script>
    
  </body>
</html>


